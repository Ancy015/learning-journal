class SubtractProductAndSum {
    public static void main(String[] args) {
        int n = 234;
        int sum = 0;
        int product = 1;
        int digit;

        while (n > 0) {
            digit = n % 10;
            sum = sum + digit;
            product = product * digit;
            n = n / 10;
        }

        int result = product - sum;
        System.out.println("Output: " + result);
    }
}

